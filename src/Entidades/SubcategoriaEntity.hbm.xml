<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entidades.SubcategoriaEntity" table="subcategoria" schema="oda">
        <id name="idSubcategoria">
            <column name="id_subcategoria" sql-type="int(11)"/>
        </id>
        <property name="nombreSubcategoria">
            <column name="nombre_subcategoria" sql-type="varchar(100)" length="100"/>
        </property>
    </class>
</hibernate-mapping>