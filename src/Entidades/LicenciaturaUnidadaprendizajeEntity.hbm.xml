<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entidades.LicenciaturaUnidadaprendizajeEntity" table="licenciatura_unidadaprendizaje" schema="oda">
        <id name="idLicenciatura">
            <column name="id_licenciatura" sql-type="int(11)"/>
        </id>
        <property name="nombreLicenciatura">
            <column name="nombre_licenciatura" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="nombreUnidadaprendizaje">
            <column name="nombre_unidadaprendizaje" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>