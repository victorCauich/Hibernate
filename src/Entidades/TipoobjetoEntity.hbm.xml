<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entidades.TipoobjetoEntity" table="tipoobjeto" schema="oda">
        <id name="idTipoobjeto">
            <column name="id_tipoobjeto" sql-type="int(11)"/>
        </id>
        <property name="nombreTipoobjeto">
            <column name="nombre_tipoobjeto" sql-type="varchar(10)" length="10"/>
        </property>
    </class>
</hibernate-mapping>